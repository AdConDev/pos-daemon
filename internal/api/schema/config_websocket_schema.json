{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "data": {
      "type": "object",
      "properties": {
        "Printer": {
          "type": "string",
          "description": "Nombre de la impresora configurada"
        },
        "DebugLog": {
          "type": "boolean",
          "description": "Indica si el registro de depuración está habilitado"
        }
      },
      "required": ["Printer", "DebugLog"]
    }
  },
  "required": ["data"]
}